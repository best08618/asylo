#ifndef THIRD_PARTY_ASYLO_PLATFORM_SYSTEM_INCLUDE_STRING_
#define THIRD_PARTY_ASYLO_PLATFORM_SYSTEM_INCLUDE_STRING_

#include_next <string>

// If we are not using the Google-internal version of <string> then add
// std::string to the global namespace for compatibility with Google coding
// conventions.
#ifndef HAS_GLOBAL_STRING
using std::string;
#endif

namespace std {

// Unimplemented stub
string to_string(int i);

}

#endif  // THIRD_PARTY_ASYLO_PLATFORM_SYSTEM_INCLUDE_STRING_
